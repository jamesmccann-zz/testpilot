<% if (user.full_name) { %>
  <%= user.full_name %>
<% } else { %>
  <em><%= user.email + " (invited)" %></em>
<% } %>
<button class="btn btn-xs btn-danger pull-right js-remove-assignment" data-confirm="Are you sure you would like to revoke <%= user.email %>'s access to this application?">
  <i class="fa fa-minus-square"></i>&nbsp;
  Revoke Access
</button>

